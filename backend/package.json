{
  "name": "backend",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "cd ../client && npm run build && cd ../backend && node server.js",
    "dev": "concurrently \"cd ../client && npm start\" \"nodemon server.js\"",
    "production": "cd ../client && npm run build && cd ../backend && cross-env NODE_ENV=production node server.js"
  },
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "concurrently": "^6.3.0",
    "cookie-parser": "~1.4.4",
    "cross-env": "^7.0.3",
    "debug": "~2.6.9",
    "express": "~4.16.1",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.0.12",
    "morgan": "~1.9.1",
    "nodemon": "^2.0.14",
    "web-push": "^3.4.5"
  },
  "type": "module"
}
